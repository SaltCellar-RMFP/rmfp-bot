{
	"$schema": "https://json.schemastore.org/tsconfig.json",
	"extends": "@sapphire/ts-config/extra-strict",
	"compilerOptions": {
		"declaration": false,
		"declarationMap": false,
		"sourceMap": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"target": "ESNext",
		"outDir": "dist",
		"skipLibCheck": true,
		"strictNullChecks": true,
	},
}
